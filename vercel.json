{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "claude/create-web-app-011CUaHgrMQMuXwQF1McEAhv": true
    }
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "crossfitleiden.com"
        }
      ],
      "destination": "https://www.crossfitleiden.com/:path*",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "accept-language",
          "value": "(.*nl.*)"
        }
      ],
      "destination": "/nl",
      "permanent": false
    },
    {
      "source": "/",
      "destination": "/en",
      "permanent": false
    }
  ]
}
